<svg width="200" height="200" viewBox="0 0 200 200" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="circuitGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#32CD32;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#228B22;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="leafGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#F5F5DC;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#DDD;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="mountainGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#8B4513;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#D2691E;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Círculo externo (madeira) -->
  <circle cx="100" cy="100" r="95" fill="none" stroke="#8B4513" stroke-width="10"/>
  <circle cx="100" cy="100" r="85" fill="none" stroke="#CD853F" stroke-width="3"/>
  
  <!-- Montanhas (background) -->
  <path d="M 30 120 L 60 80 L 90 100 L 120 70 L 150 90 L 170 120 Z" fill="url(#mountainGradient)" opacity="0.8"/>
  
  <!-- Circuitos tecnológicos -->
  <g stroke="url(#circuitGradient)" stroke-width="2" fill="none">
    <!-- Circuitos da esquerda -->
    <path d="M 20 140 L 40 140 L 40 130 L 60 130"/>
    <circle cx="60" cy="130" r="2" fill="#32CD32"/>
    <path d="M 20 150 L 50 150 L 50 145 L 70 145"/>
    <circle cx="70" cy="145" r="2" fill="#32CD32"/>
    
    <!-- Circuitos da direita -->
    <path d="M 180 140 L 160 140 L 160 130 L 140 130"/>
    <circle cx="140" cy="130" r="2" fill="#32CD32"/>
    <path d="M 180 150 L 150 150 L 150 145 L 130 145"/>
    <circle cx="130" cy="145" r="2" fill="#32CD32"/>
    
    <!-- Circuitos centrais -->
    <path d="M 100 170 L 100 155 L 120 155 L 120 165"/>
    <circle cx="120" cy="165" r="2" fill="#32CD32"/>
    <path d="M 100 170 L 100 160 L 80 160 L 80 165"/>
    <circle cx="80" cy="165" r="2" fill="#32CD32"/>
  </g>
  
  <!-- Folha central (representa o crescimento) -->
  <g transform="translate(100,100)">
    <!-- Caule -->
    <rect x="-2" y="0" width="4" height="40" fill="#8B4513"/>
    
    <!-- Folha -->
    <ellipse cx="0" cy="-20" rx="25" ry="35" fill="url(#leafGradient)" stroke="#8B4513" stroke-width="2"/>
    
    <!-- Nervuras da folha -->
    <path d="M 0 -50 L 0 10" stroke="#8B4513" stroke-width="2"/>
    <path d="M 0 -35 L -15 -25" stroke="#8B4513" stroke-width="1"/>
    <path d="M 0 -35 L 15 -25" stroke="#8B4513" stroke-width="1"/>
    <path d="M 0 -20 L -20 -15" stroke="#8B4513" stroke-width="1"/>
    <path d="M 0 -20 L 20 -15" stroke="#8B4513" stroke-width="1"/>
    <path d="M 0 -10 L -15 -5" stroke="#8B4513" stroke-width="1"/>
    <path d="M 0 -10 L 15 -5" stroke="#8B4513" stroke-width="1"/>
  </g>
  
  <!-- Texto integrado -->
  <text x="100" y="185" text-anchor="middle" font-family="'Roboto Slab', serif" font-size="12" font-weight="bold" fill="#8B4513">
    CalangoFlux
  </text>
</svg>